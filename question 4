// Create two objects with properties name and age
const person1 = {
    name: 'John',
    age: 30
};

const person2 = {
    name: 'Jane',
    age: 25
};

// Create the introduce function
function introduce() {
    console.log(`Hello, I'm ${this.name}, and I'm ${this.age} years old.`);
}

// Use the call method to make each person introduce themselves
introduce.call(person1); // Output: Hello, I'm John, and I'm 30 years old.
introduce.call(person2); // Output: Hello, I'm Jane, and I'm 25 years old.
